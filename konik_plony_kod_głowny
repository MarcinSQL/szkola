                    l62 = 0;
                    l63 = 0;
                    l64 = 0;
                }

                l12 = 1;
                l19 = 1;
                l17 = 1;
                if (label2.Text == "")
                {
                    label2.Text = Convert.ToString(++n);
                    wynik.Text = ("Wynik: " + Convert.ToString(++w));

                    if (label12.Text == "" || label19.Text == "" || label7.Text == "")
                    {

                    }
                    else
                    {
                        koniec.Text = "Koniec gry!";
                        l2 = 2;
