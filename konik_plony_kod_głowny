        private void label2_Click(object sender, EventArgs e)
        {
            if (w == 0)
            {
                wynik.Text = ("Wynik: " + Convert.ToString(++w));
                label2.Text = Convert.ToString(++n);
                l12 = 1;
                l19 = 1;
                l17 = 1;

            }
            else if (l2 == 1)
            {
                {
                    l1 = 0;
                    l2 = 0;
                    l3 = 0;
                    l4 = 0;
                    l5 = 0;
                    l6 = 0;
                    l7 = 0;
                    l8 = 0;
                    l9 = 0;
                    l10 = 0;
                    l11 = 0;
