                    l57 = 0;
                    l58 = 0;
                    l59 = 0;
                    l60 = 0;
                    l61 = 0;
                    l62 = 0;
                    l63 = 0;
                    l64 = 0;
                }

                l11 = 1;
                l18 = 1;
                if (label1.Text=="")
                {
                    label1.Text = Convert.ToString(++n);
                    wynik.Text = ("Wynik: " + Convert.ToString(++w));

                    if (label11.Text == "" || label18.Text == "")
                    {

                    }
                    else
                    {
                        koniec.Text = "Koniec gry!";
                        l1 = 2;
                        if (cofnijToolStripMenuItem.Enabled == true)
                        {
                            mozc.Text = "Masz możliwość cofnięcia ruchu!";
                        }
                    }
                }
            }
        }
    }
}
